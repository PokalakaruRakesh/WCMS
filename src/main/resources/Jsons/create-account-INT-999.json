{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Objective": "No Objective specified",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email address",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit account creation form",
        "locatorType": "id",
        "locatorValue": "register-submit"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser123",
        "email": "testuser123@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "newemail@example.com",
        "password": "AnotherPassw0rd!",
        "confirmPassword": "AnotherPassw0rd!",
        "expectedError": "Username already exists."
      },
      {
        "scenario": "Invalid email format",
        "username": "newuser",
        "email": "invalid-email",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expectedError": "Please enter a valid email address."
      },
      {
        "scenario": "Password and confirm password do not match",
        "username": "newuser2",
        "email": "newuser2@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "MismatchPassw0rd!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Password too short (boundary)",
        "username": "shortpassuser",
        "email": "shortpass@example.com",
        "password": "123",
        "confirmPassword": "123",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      },
      {
        "scenario": "Email already registered",
        "username": "uniqueuser",
        "email": "existingemail@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expectedError": "Email already registered."
      },
      {
        "scenario": "Username at max length (boundary)",
        "username": "usernamemaxlength1234567890",
        "email": "maxuser@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password with special characters",
        "username": "specialcharsuser",
        "email": "specialchars@example.com",
        "password": "P@$$w0rd!#%&*()",
        "confirmPassword": "P@$$w0rd!#%&*()",
        "expectedConfirmation": "Your account has been created successfully."
      }
    ],
    "EdgeCases": [
      {
        "description": "Attempt to create account with SQL injection in username",
        "username": "testuser'; DROP TABLE users;--",
        "email": "sqlinject@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expected": "Input validation error or generic failure message."
      },
      {
        "description": "Attempt to create account with XSS in email",
        "username": "xssuser",
        "email": "<script>alert('xss')</script>@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expected": "Input validation error or generic failure message."
      },
      {
        "description": "Attempt to create account with whitespace-only username",
        "username": "     ",
        "email": "whitespace@example.com",
        "password": "ValidPassw0rd!",
        "confirmPassword": "ValidPassw0rd!",
        "expected": "Input validation error or generic failure message."
      },
      {
        "description": "Attempt to create account with maximum allowed field lengths",
        "username": "u".repeat(50),
        "email": "maxfieldlengthuser@example.com",
        "password": "P".repeat(50),
        "confirmPassword": "P".repeat(50),
        "expected": "Your account has been created successfully."
      }
    ]
  }
}
