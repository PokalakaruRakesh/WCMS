{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Objective": "No Objective specified",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email address",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit the registration form",
        "locatorType": "xpath",
        "locatorValue": "//button[@type='submit' and contains(text(),'Create Account')]"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser001",
        "email": "testuser001@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "existinguser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Username already exists. Please choose another."
      },
      {
        "scenario": "Invalid email format",
        "username": "testuser002",
        "email": "invalid-email",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Please enter a valid email address."
      },
      {
        "scenario": "Password and confirm password do not match",
        "username": "testuser003",
        "email": "testuser003@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "DifferentPass123!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Password below minimum length",
        "username": "testuser004",
        "email": "testuser004@example.com",
        "password": "123",
        "confirmPassword": "123",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      },
      {
        "scenario": "Boundary value: username max length",
        "username": "usernamemaxlen1234567890",
        "email": "maxlenuser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Boundary value: username min length",
        "username": "us",
        "email": "minlenuser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Username must be at least 3 characters."
      }
    ],
    "EdgeCases": [
      {
        "description": "Attempt to register with SQL injection in username",
        "username": "testuser'; DROP TABLE users;--",
        "email": "sqlinject@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Input validation error or sanitized input."
      },
      {
        "description": "Attempt to register with XSS in email field",
        "username": "testuserxss",
        "email": "<script>alert('xss')</script>@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Input validation error or sanitized input."
      },
      {
        "description": "Attempt to register with maximum allowed field lengths",
        "username": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa", 
        "email": "longemailaddresslongemailaddresslongemailaddresslongemailaddresslongemailaddress@example.com",
        "password": "VeryLongPassword1234567890!@#$%^&*()_+",
        "confirmPassword": "VeryLongPassword1234567890!@#$%^&*()_+",
        "expected": "Account should be created successfully or field length error message."
      }
    ]
  }
}
