{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Objective": "No Objective specified",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email address",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit the registration form",
        "locatorType": "id",
        "locatorValue": "register-submit"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser001",
        "email": "testuser001@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Username at minimum length",
        "username": "us",
        "email": "minuser@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Username at maximum length",
        "username": "usernamemaxlength20chr",
        "email": "maxuser@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password at minimum length",
        "username": "minpassuser",
        "email": "minpass@example.com",
        "password": "Passw1!",
        "confirmPassword": "Passw1!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password at maximum length",
        "username": "maxpassuser",
        "email": "maxpass@example.com",
        "password": "Password@12345678901234567890",
        "confirmPassword": "Password@12345678901234567890",
        "expectedConfirmation": "Your account has been created successfully."
      }
    ],
    "EdgeCases": [
      {
        "description": "Mismatched passwords",
        "username": "mismatchuser",
        "email": "mismatch@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@12345",
        "expectedError": "Passwords do not match."
      },
      {
        "description": "Invalid email format",
        "username": "invalidemailuser",
        "email": "invalidemail.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedError": "Please enter a valid email address."
      },
      {
        "description": "Username already exists",
        "username": "testuser001",
        "email": "newemail@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedError": "Username already exists."
      },
      {
        "description": "Email already registered",
        "username": "newuser",
        "email": "testuser001@example.com",
        "password": "Password@1234",
        "confirmPassword": "Password@1234",
        "expectedError": "Email address already registered."
      },
      {
        "description": "Password missing special character",
        "username": "nospecialcharuser",
        "email": "nospecial@example.com",
        "password": "Password1234",
        "confirmPassword": "Password1234",
        "expectedError": "Password must contain at least one special character."
      },
      {
        "description": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      }
    ]
  }
}
