{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Objective": "No Objective specified",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit account creation",
        "locatorType": "id",
        "locatorValue": "register-submit"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser01",
        "email": "testuser01@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "existinguser@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Username already exists."
      },
      {
        "scenario": "Invalid email format",
        "username": "newuser02",
        "email": "invalid-email",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Please enter a valid email address."
      },
      {
        "scenario": "Password and confirm password mismatch",
        "username": "newuser03",
        "email": "newuser03@example.com",
        "password": "Password123!",
        "confirmPassword": "Password1234!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Password too short",
        "username": "newuser04",
        "email": "newuser04@example.com",
        "password": "Pass1!",
        "confirmPassword": "Pass1!",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      },
      {
        "scenario": "Boundary value for username (min length)",
        "username": "us",
        "email": "us@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Username must be at least 3 characters."
      },
      {
        "scenario": "Boundary value for username (max length)",
        "username": "usernamemaxlengthtestusernamemaxlengthtest",
        "email": "usernamemax@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedConfirmation": "Your account has been created successfully."
      }
    ],
    "EdgeCases": [
      {
        "description": "Attempt to create account with SQL injection in username",
        "username": "testuser'; DROP TABLE users;--",
        "email": "sqlinject@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Input validation error."
      },
      {
        "description": "Account creation with special characters in username",
        "username": "user!@#",
        "email": "specialchar@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Your account has been created successfully."
      },
      {
        "description": "Account creation with unicode characters in username",
        "username": "用户测试",
        "email": "unicodeuser@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Your account has been created successfully."
      },
      {
        "description": "Rapid multiple submissions (double click submit)",
        "username": "rapiduser",
        "email": "rapiduser@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Only one account is created and confirmation is shown once."
      }
    ]
  }
}
