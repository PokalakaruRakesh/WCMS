    "EdgeCases": [
      {
        "description": "Link is present but not clickable (disabled state)",
        "expected": "Link should not be counted as working; report as not accessible."
      },
      {
        "description": "Link points to a 404 or broken page",
        "expected": "Test should fail and log the broken link."
      },
      {
        "description": "Button triggers a modal or dynamic content",
        "expected": "Test should verify the modal/dynamic content appears as expected."
      },
      {
        "description": "Link opens in a new tab/window",
        "expected": "Test should verify the new tab/window opens and the URL is correct."
      },
      {
        "description": "Link with JavaScript handler only (no href)",
        "expected": "Test should verify the correct JS action is triggered."
      },
      {
        "description": "Multiple links with same text but different URLs",
        "expected": "Test should verify each link instance separately."
      },
      {
        "description": "Links/buttons hidden behind dropdowns or accordions",
        "expected": "Test should expand dropdowns/accordions and verify all links/buttons."
      },
      {
        "description": "Very long link text or button text (boundary value)",
        "expected": "Test should verify UI handles long text gracefully and link/button is accessible."
      },
      {
        "description": "Links/buttons with special characters in text",
        "expected": "Test should verify correct rendering and accessibility."
      },
      {
        "description": "No links/buttons present on the page",
        "expected": "Test should report as a failure (unexpected state)."
      }
