    "EdgeCases": [
      {
        "description": "YouTube link is broken (404)",
        "expected": "Test should fail and log broken link error"
      },
      {
        "description": "LinkedIn page is private or access restricted",
        "expected": "Test should fail and log access restriction"
      },
      {
        "description": "Social icon is missing from footer",
        "expected": "Test should fail and log missing element"
      },
      {
        "description": "YouTube/LinkedIn opens in same tab instead of new tab",
        "expected": "Test should fail and log incorrect target attribute"
      },
      {
        "description": "LinkedIn company page loads but company info section missing",
        "expected": "Test should fail and log missing company info section"
      },
      {
        "description": "YouTube channel loads but no videos present",
        "expected": "Test should fail and log missing videos section"
      },
      {
        "description": "LinkedIn page loads with login prompt",
        "expected": "Test should fail and log login prompt present"
      },
      {
        "description": "YouTube/LinkedIn icon is present but not clickable",
        "expected": "Test should fail and log not clickable"
      },
      {
        "description": "YouTube/LinkedIn icon color does not match brand",
        "expected": "Test should fail and log color mismatch"
      },
      {
        "description": "Multiple YouTube/LinkedIn icons present (duplicates)",
