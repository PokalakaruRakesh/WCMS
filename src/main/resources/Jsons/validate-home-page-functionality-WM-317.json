    "EdgeCases": [
      {
        "description": "Button is visible but disabled",
        "buttonName": "Join Now",
        "isEnabled": false,
        "expected": "Button should not be clickable and no navigation should occur."
      },
      {
        "description": "Link is missing href attribute",
        "linkText": "Privacy Policy",
        "expected": "Link should not navigate anywhere and should be reported as broken."
      },
      {
        "description": "Button/link is present but not visible",
        "buttonName": "Download Brochure",
        "isVisible": false,
        "expected": "Button should not be interactable and should be reported as hidden."
      },
      {
        "description": "Link points to a 404 page",
        "linkText": "Sitemap",
        "expectedNavigationUrl": "/sitemap",
        "expected": "Navigation should result in 404 error and should be reported as broken link."
      },
      {
        "description": "Multiple links with same text",
        "linkText": "Contact",
        "expected": "All links should be checked for correct navigation and uniqueness."
      },
      {
        "description": "Button triggers modal instead of navigation",
        "buttonName": "Subscribe",
        "expected": "Modal should appear and be validated for correct content."
      },
      {
        "description": "Link opens in a new tab",
        "linkText": "LinkedIn",
        "expectedNavigationUrl": "https://linkedin.com/company/astm",
        "expected": "Link should open in new tab and navigate to correct URL."
      },
      {
        "description": "Button/link with slow response",
