{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Type": "Functional",
    "Objective": "Test the account creation process with various valid and invalid inputs",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit the registration form",
        "locatorType": "id",
        "locatorValue": "register-submit"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser01",
        "email": "testuser01@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedConfirmation": "Account created successfully."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "newemail@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Username already exists."
      },
      {
        "scenario": "Email already registered",
        "username": "uniqueuser",
        "email": "existingemail@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Email already registered."
      },
      {
        "scenario": "Password and confirm password do not match",
        "username": "user02",
        "email": "user02@example.com",
        "password": "Password123!",
        "confirmPassword": "Password321!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Invalid email format",
        "username": "user03",
        "email": "invalid-email",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Invalid email format."
      },
      {
        "scenario": "Password too short",
        "username": "user04",
        "email": "user04@example.com",
        "password": "123",
        "confirmPassword": "123",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Username at max length",
        "username": "usernamemaxlength1234567890",
        "email": "usermax@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedConfirmation": "Account created successfully."
      },
      {
        "scenario": "Username at min length",
        "username": "us",
        "email": "us@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Username must be at least 3 characters."
      },
      {
        "scenario": "All fields empty",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      },
      {
        "scenario": "Special characters in username",
        "username": "user!@#",
        "email": "specialchar@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expectedError": "Username cannot contain special characters."
      }
    ],
    "EdgeCases": [
      {
        "description": "Username with leading/trailing spaces",
        "username": "  user05  ",
        "email": "user05@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Trimmed username should be accepted if valid."
      },
      {
        "description": "Email with uppercase letters",
        "username": "user06",
        "email": "USER06@EXAMPLE.COM",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Email should be treated as case-insensitive and accepted."
      },
      {
        "description": "Password with all allowed special characters",
        "username": "user07",
        "email": "user07@example.com",
        "password": "P@ssw0rd!#%&*",
        "confirmPassword": "P@ssw0rd!#%&*",
        "expected": "Account should be created successfully."
      },
      {
        "description": "SQL injection in username",
        "username": "user08'; DROP TABLE users;--",
        "email": "user08@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Input should be sanitized and error displayed."
      },
      {
        "description": "Extremely long email address",
        "username": "user09",
        "email": "verylongemailaddressverylongemailaddressverylongemailaddressverylongemailaddress@example.com",
        "password": "Password123!",
        "confirmPassword": "Password123!",
        "expected": "Error or truncation as per system limits."
      }
    ]
  }
}
