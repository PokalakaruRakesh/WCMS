{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Type": "Functional",
    "Objective": "Test the account creation process with valid and invalid inputs.",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "register-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email address",
        "locatorType": "id",
        "locatorValue": "register-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "register-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field to confirm password",
        "locatorType": "id",
        "locatorValue": "register-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit account creation form",
        "locatorType": "css",
        "locatorValue": "button[type='submit']"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser1",
        "email": "testuser1@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password mismatch",
        "username": "testuser2",
        "email": "testuser2@example.com",
        "password": "Password123!",
        "confirmPassword": "Password321!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Invalid email format",
        "username": "testuser3",
        "email": "invalid-email",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Please enter a valid email address."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "existinguser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Username already exists."
      },
      {
        "scenario": "Password too short (boundary)",
        "username": "testuser4",
        "email": "testuser4@example.com",
        "password": "123",
        "confirmPassword": "123",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      },
      {
        "scenario": "Password missing special character",
        "username": "testuser5",
        "email": "testuser5@example.com",
        "password": "Password123",
        "confirmPassword": "Password123",
        "expectedError": "Password must contain at least one special character."
      },
      {
        "scenario": "Email already registered",
        "username": "uniqueuser",
        "email": "existinguser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Email already registered."
      }
    ],
    "EdgeCases": [
      {
        "description": "Username at maximum length (50 chars)",
        "username": "user_abcdefghijklmnopqrstuvwxyz_ABCDEFGHIJKLMNOPQRSTUVWXYZ_123456",
        "email": "maxuser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Account should be created successfully."
      },
      {
        "description": "Username at minimum length (3 chars)",
        "username": "abc",
        "email": "minuser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Account should be created successfully."
      },
      {
        "description": "Password at maximum length (100 chars)",
        "username": "testuser6",
        "email": "testuser6@example.com",
        "password": "P@ssw0rd" + "A".repeat(92),
        "confirmPassword": "P@ssw0rd" + "A".repeat(92),
        "expected": "Account should be created successfully."
      },
      {
        "description": "SQL injection in username field",
        "username": "testuser'; DROP TABLE users;--",
        "email": "sqluser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Input should be sanitized and account created if valid."
      },
      {
        "description": "XSS attack in email field",
        "username": "xssuser",
        "email": "<script>alert('xss')</script>@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Input should be sanitized and error shown for invalid email."
      }
    ]
  }
}
