{
  "INT-999": {
    "TestCaseID": "INT-999",
    "TestCaseName": "Create account",
    "Priority": "Medium",
    "Type": "Functional",
    "Objective": "To verify the functionality of the Create Account page",
    "ExpectedResult": "Account should be created successfully and confirmation message displayed",
    "Locators": [
      {
        "name": "usernameField",
        "description": "Input field for username",
        "locatorType": "id",
        "locatorValue": "create-account-username"
      },
      {
        "name": "emailField",
        "description": "Input field for email address",
        "locatorType": "id",
        "locatorValue": "create-account-email"
      },
      {
        "name": "passwordField",
        "description": "Input field for password",
        "locatorType": "id",
        "locatorValue": "create-account-password"
      },
      {
        "name": "confirmPasswordField",
        "description": "Input field for confirming password",
        "locatorType": "id",
        "locatorValue": "create-account-confirm-password"
      },
      {
        "name": "submitButton",
        "description": "Button to submit the create account form",
        "locatorType": "css",
        "locatorValue": "button#create-account-submit"
      }
    ],
    "TestData": [
      {
        "scenario": "Valid account creation",
        "username": "testuser001",
        "email": "testuser001@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password mismatch",
        "username": "testuser002",
        "email": "testuser002@example.com",
        "password": "Password1!",
        "confirmPassword": "Password2!",
        "expectedError": "Passwords do not match."
      },
      {
        "scenario": "Invalid email format",
        "username": "testuser003",
        "email": "invalid-email-format",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Please enter a valid email address."
      },
      {
        "scenario": "Username already exists",
        "username": "existinguser",
        "email": "existinguser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expectedError": "Username already exists."
      },
      {
        "scenario": "Password too short (boundary)",
        "username": "testuser004",
        "email": "testuser004@example.com",
        "password": "Ab1!",
        "confirmPassword": "Ab1!",
        "expectedError": "Password must be at least 8 characters."
      },
      {
        "scenario": "Password at minimum length (boundary)",
        "username": "testuser005",
        "email": "testuser005@example.com",
        "password": "Abcdef1!",
        "confirmPassword": "Abcdef1!",
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Password at maximum length (boundary)",
        "username": "testuser006",
        "email": "testuser006@example.com",
        "password": "A1!" + "a".repeat(57),
        "confirmPassword": "A1!" + "a".repeat(57),
        "expectedConfirmation": "Your account has been created successfully."
      },
      {
        "scenario": "Empty required fields",
        "username": "",
        "email": "",
        "password": "",
        "confirmPassword": "",
        "expectedError": "All fields are required."
      }
    ],
    "EdgeCases": [
      {
        "description": "Username with special characters",
        "username": "user!@#",
        "email": "user_special@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Username cannot contain special characters."
      },
      {
        "description": "Email already registered",
        "username": "uniqueuser",
        "email": "existinguser@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Email already registered."
      },
      {
        "description": "Password missing uppercase letter",
        "username": "testuser007",
        "email": "testuser007@example.com",
        "password": "lowercase123!",
        "confirmPassword": "lowercase123!",
        "expected": "Password must contain at least one uppercase letter."
      },
      {
        "description": "Password missing number",
        "username": "testuser008",
        "email": "testuser008@example.com",
        "password": "NoNumbers!",
        "confirmPassword": "NoNumbers!",
        "expected": "Password must contain at least one number."
      },
      {
        "description": "Password missing special character",
        "username": "testuser009",
        "email": "testuser009@example.com",
        "password": "NoSpecial123",
        "confirmPassword": "NoSpecial123",
        "expected": "Password must contain at least one special character."
      },
      {
        "description": "Whitespace in username",
        "username": "user name",
        "email": "whitespace@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Username cannot contain spaces."
      },
      {
        "description": "SQL injection attempt in username",
        "username": "testuser'; DROP TABLE users;--",
        "email": "sqlinject@example.com",
        "password": "ValidPass123!",
        "confirmPassword": "ValidPass123!",
        "expected": "Invalid username."
      }
    ]
  }
}
